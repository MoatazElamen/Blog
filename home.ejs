<% include header %>
<h1>Welcome to learn about me!</h1>
<% users.forEach((user)=>{ %>
<div class="panel panel-default">
    <div class="panel-heading"> <a href="/user/<%= user.username %> ">
    <%= user.name() %>
    </a></div>
    <% if (user.bio) { %>
    <div class="panel-body"><%= user.bio %></div>
    <%}%>
</div>
<%})%>

<% include footer %>